<h1>Listing pdf_records</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Content</th>
    <th>Pdf file name</th>
    <th>Pdf file size</th>
    <th>Pdf content type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pdf_records.each do |pdf_record| %>
  <tr>
    <td><%= pdf_record.name %></td>
    <td><%= pdf_record.content %></td>
    <td><%= pdf_record.pdf_file_name %></td>
    <td><%= pdf_record.pdf_file_size %></td>
    <td><%= pdf_record.pdf_content_type %></td>
    <td><%= link_to 'Show', pdf_record %></td>
    <td><%= link_to 'Edit', edit_pdf_record_path(pdf_record) %></td>
    <td><%= link_to 'Destroy', pdf_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pdf record', new_pdf_record_path %>
